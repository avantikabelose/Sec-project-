<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Odd Numbers</title>
</head>
<body>

<h2>Odd Numbers between 1 and 100</h2>

<button onclick="printOddNumbers()">Print Odd Numbers</button>

<p id="result"></p>

<script>
    function printOddNumbers() {
        let oddNumbers = [];
        for (let i = 1; i <= 100; i += 2) {
            oddNumbers.push(i);
        }
        document.getElementById("result").innerHTML = "Odd Numbers: " + oddNumbers.join(", ");
    }
</script>

</body>
</html>
